{
  "name": "pfa-negotiations-app",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run db-migrate --env development && npm run build & concurrently --kill-others \"npm run serve-api\" \"npm run serve\"",
    "start:dev": "npm run db-migrate --env development && npm run build",
    "start:prod": "npm run db-migrate --env production && npm run build:prod",
    "start:qa": "npm run db-migrate --env testing && npm run build:prod",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "build": "npm run config -- && ng build --output-path=public",
    "build:prod": "npm run config -- && ng build --aot --prod",
    "serve-api": "node app.js",
    "config": "ts-node ./src/config-env.ts",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "db-migrate": "node_modules/.bin/sequelize db:migrate",
    "db-undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db-clear": "node_modules/.bin/sequelize db:migrate:undo:all",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/pfa-negotiations-app/stats.json --port 8880"
  },
  "private": true,
